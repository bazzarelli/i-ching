{
    "name": "i-ching-for-beginners",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://bazzarelli.github.io/i-ching/",
    "dependencies": {
        "classnames": "^2.2.6",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-router": "^5.0.0",
        "react-router-dom": "^5.0.0",
        "react-scripts": "2.1.8"
    },
    "scripts": {
        "build:css": "postcss src/styles/index.css -o src/index.css",
        "watch:css": "postcss src/styles/index.css -o src/index.css -w",
        "start": "npm run watch:css & react-scripts start",
        "build": "npm run build:css && react-scripts build && rm -rf docs && mv build docs",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "autoprefixer": "^9.4.10",
        "postcss-cli": "^6.1.2",
        "tailwindcss": "^0.7.4"
    }
}
